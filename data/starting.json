{
 "configs": [
  "/config.options.json",
  "/config.setup.json",
  "/lang/lang.{{lang}}.json",
  "/modules.json"
 ],
 "title": "Starting Log",
 "class":"col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6",
 "content": [
  {
   "type": "h5",
   "title": "{{SSDP}}",
   "class":"alert-default"
  },

  {
   "type": "loadJson",
   "state": "header.json"
  },

  {
   "type": "h2",
   "title": "{{LangType}} {{configs}}"
  },
  {
   "type": "text",
   "title":"Обратите внимание на данный момент время показывается в GMT 0 зоне.",
   "class": "alert alert-info"
  },


  {
   "type": "table",
   "state": "http://backup.privet.lv/visitors/{{mac}}.json",
   "title": {
    "starting":"Starting",
    "startingInfo":"Starting Info",
    "module":"Module"
   },
   "class": "table table-striped table-hover",
   "style": "width:100%;"
  },

  {
   "type": "hr"
  },
  {
   "type": "link",
   "title": "<i class=\"opt-img\"></i> {{LangSetting3}}",
   "action": "/page.htm?setup",
   "class": "btn btn-block btn-default"
  }

 ]
}
